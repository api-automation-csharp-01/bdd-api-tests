{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "default": {},
  "required": [
    "id",
    "name",
    "comment_count",
    "color",
    "shared",
    "sync_id",
    "order",
    "favorite"
  ],
  "properties": {
    "id": {

      "type": "integer",
      "title": "The id schema",
      "default": 0
    },
    "name": {

      "type": "string",
      "title": "The name schema",
      "default": ""
    },
    "comment_count": {

      "type": "integer",
      "title": "The comment_count schema",
      "default": 0
    },
    "color": {

      "type": "integer",
      "title": "The color schema",
      "default": 0
    },
    "shared": {

      "type": "boolean",
      "title": "The shared schema",
      "default": false
    },
    "sync_id": {

      "type": "integer",
      "title": "The sync_id schema",
      "default": 0
    },
    "order": {

      "type": "integer",
      "title": "The order schema",
      "default": 0
    },
    "favorite": {

      "type": "boolean",
      "title": "The favorite schema",
      "default": false
    }
  }
}