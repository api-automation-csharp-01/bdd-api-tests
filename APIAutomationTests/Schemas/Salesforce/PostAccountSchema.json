{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "id",
    "success",
    "errors"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string"
    },
    "success": {
      "$id": "#/properties/success",
      "type": "boolean"
    },
    "errors": {
      "$id": "#/properties/errors",
      "type": "array",
      "additionalItems": true,
      "items": {
        "$id": "#/properties/errors/items"
      }
    }
  },
  "additionalProperties": true
}